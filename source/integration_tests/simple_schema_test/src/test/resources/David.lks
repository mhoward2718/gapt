proof \psi  proves
    0 < c(k+1), B(k+1,p,y(k+1)), (c(k+1) * EXP(k+1,p)) = y(k+1) |-


base {
    1:      ax( 0 < c(1), b(0) < p |- b(0) < (c(1) * p) )
    2:      weakL( 1, 0 <= b(0) )
    3:      andL( 2, 0 <= b(0), b(0) < p)
    4:      ax( b(0) < (c(1) * p) |- 0 < ((c(1) * p) - b(0)) )
    5:      cut(3, 4, b(0) < (c(1) * p) )

    6:      ax( (c(1) * p) =  (b(0) + 0) |- ((c(1) * p) - b(0)) = 0 )
    7:      andR(6, 5, ((c(1) * p) - b(0)) = 0 , 0 < ((c(1) * p) - b(0)))
    8:      exR(7, (((c(1) * p) - b(0)) = 0 /\  0 < ((c(1) * p) - b(0))) , Exists x ( x = 0 /\ 0 < x) , ((c(1) * p) - b(0)) )
    9:      ax( c(0) = 0 , 0 < c(0) |-)
    10:     andL( 9, c(0) = 0 , 0 < c(0))
    11:	    exL(10 , (c(0) = 0 /\ 0 < c(0)), Exists x ( x = 0 /\ 0 < x), c(0))
    12:     cut(8, 11, Exists x (( x = 0 /\ 0 < x) ))

    13:     ax( y(1) = (b(0) + 0) , (c(1) * p) = y(1) |- (c(1) * p) = (b(0) + 0) )
    14:     cut(13, 12, (c(1) * p) = (b(0) + 0) )
    15:     andL( 14, (0 <= b(0) /\ b(0) < p) , y(1) = (b(0) + 0) )
    root:   exL(15 , ((0 <= b(0) /\ b(0) < p)  /\  y(1) = (b(0) + 0)), Exists v ( (0 <= v /\ v < p)  /\  y(1) = (v + 0)), b(0))
}

step {
    1:      ax( 0 < c(k+2), b(k+1) < p |- b(k+1) < (c(k+2) * p) )
    2:      weakL( 1, 0 <= b(k+1) )
    3:      andL( 2, 0 <= b(k+1), b(k+1) < p)
    4:      ax( b(k+1) < (c(k+2) * p) |- 0 < ((c(k+2) * p) - b(k+1)) )
    5:      cut(3, 4, b(k+1) < (c(k+2) * p) )
    6:      ax( (c(k+2) * EXP(k+2, p)) = ((b(k+1) * EXP(k+1, p)) + y(k+1))
                   |-
                ((c(k+2) * EXP(k+2, p)) - (b(k+1) * EXP(k+1, p))) = y(k+1))

    7:      ax( ((c(k+2) * EXP(k+2, p)) - (b(k+1) * EXP(k+1, p))) = y(k+1)
                               |-
                (((c(k+2) * p) - b(k+1)) * EXP(k+1, p)) = y(k+1) )
    8:      cut(6, 7, ((c(k+2) * EXP(k+2, p)) - (b(k+1) * EXP(k+1, p))) = y(k+1) )

    9:      andR(8, 5, (((c(k+2) * p) - b(k+1)) * EXP(k+1, p)) = y(k+1), 0 < ((c(k+2) * p) - b(k+1)) )
    10:     exR(9, ((((c(k+2) * p) - b(k+1)) * EXP(k+1, p)) = y(k+1) /\ 0 < ((c(k+2) * p) - b(k+1))) ,
                        Exists x ((x * EXP(k+1, p)) = y(k+1) /\ 0 < x) ,
                                ((c(k+2) * p) - b(k+1)) )

    11:	    pLink((\psi, k) 0 < c(k+1), B(k,p,y(k+1)), (c(k+1) * EXP(k+1,p)) = y(k+1) |- )
    12:     andL( 11,  (c(k+1) * EXP(k+1,p)) = y(k+1), 0 < c(k+1) )
    13:	    exL( 12, ((c(k+1) * EXP(k+1,p)) = y(k+1) /\ 0 < c(k+1)) , Exists x ((x * EXP(k+1, p)) = y(k+1) /\ 0 < x), c(k+1))
    14:     cut(10,13, Exists x ((x * EXP(k+1, p)) = y(k+1) /\ 0 < x))

    15:	    ax( y(k+2) = ((b(k+1) * EXP(k+1,p)) + y(k+1)) , (c(k+2) * EXP(k+2,p)) = y(k+2) |- (c(k+2) * EXP(k+2,p)) = ((b(k+1) * EXP(k+1,p)) + y(k+1)) )
    16:     cut(15,14, (c(k+2) * EXP(k+2,p)) = ((b(k+1) * EXP(k+1,p)) + y(k+1)) )
    17:     andL(16,  (0 <= b(k+1) /\ b(k+1) < p) , B(k,p,y(k+1)) )
    18:     andL(17, y(k+2) = ((b(k+1) * EXP(k+1,p)) + y(k+1)) , ((0 <= b(k+1) /\ b(k+1) < p) /\ B(k,p,y(k+1))) )
    19:	    exL( 18, (y(k+2) = ((b(k+1) * EXP(k+1,p)) + y(k+1)) /\ ((0 <= b(k+1) /\ b(k+1) < p) /\ B(k,p,y(k+1)))) ,
                Exists w ( y(k+2) = ((b(k+1) * EXP(k+1,p)) + w)      /\ ((0 <= b(k+1) /\ b(k+1) < p) /\ B(k,p,w))), y(k+1) )

    20:   exL( 19, Exists w (y(k+2) = ((b(k+1) * EXP(k+1,p)) + w) /\ ((0 <= b(k+1) /\ b(k+1) < p) /\ B(k,p,w))),
            Exists v Exists w (y(k+2) = ((v * EXP(k+1,p)) + w)  /\ ((0 <= v /\ v < p) /\ B(k,p,w))) , b(k+1))

    root: foldL( 20, Exists v Exists w (y(k+2) = ((v * EXP(k+1,p)) + w) /\ ((0 <= v /\ v < p) /\ B(k,p,w))), B(k+1,p,y(k+1)))
}
